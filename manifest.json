{
	"name": "GenderMag Tool 2015",
	"manifest_version": 2,
	"description": "Inject a sidebar on the current tab when the page action is clicked",
	"version": "0.2",
	"background": {
		"scripts": ["jquery-1.12.0.min.js", "background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png",
		"default_title": "GenderMag Tool"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-1.12.0.min.js", "script.js"],
			"css": [ "styles.css" ]
		}
	],
	"permissions": ["storage", "tabs", "<all_urls>"]
}