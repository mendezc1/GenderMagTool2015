{
	"name": "GenderMag Tool 2015",
	"manifest_version": 2,
	"description": "Inject a sidebar on the current tab when the page action is clicked",
	"version": "0.2",
	"background": {
		"scripts": ["jquery-ui-1.11.4.custom/external/jquery/jquery.js", "jquery-ui-1.11.4.custom/jquery-ui.min.js", "background.js", "popup.js"]
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "images/icon.png",
		"default_title": "GenderMag Tool"
	},
	 "icons": { 
      "16": "images/Abby-lowres.jpg" 
     }, 
	 "web_accessible_resources": [
      "images/*.jpg"
	],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-ui-1.11.4.custom/external/jquery/jquery.js", "jquery-ui-1.11.4.custom/jquery-ui.min.js", "script.js"],
			"css": [ "styles/styles.css" ]
		}
	],
	"permissions": ["storage", "tabs", "<all_urls>"]
}